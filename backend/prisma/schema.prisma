// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Products {
  id                Int    @id @default(autoincrement())
  product           String
  product_price     Float
  product_quantity  Int

  @@map("products")
}